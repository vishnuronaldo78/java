import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		String str = "";
		int count = 0;
		for(int i = 0; i <= n; i++) {
		    str = Integer.toBinaryString(i);
		    for(int j = 0; j < str.length();j++) {
		        if(str.charAt(j)==49) {
		            count++;
		        }
		    }
		}
		System.out.println(count);
	}
}
